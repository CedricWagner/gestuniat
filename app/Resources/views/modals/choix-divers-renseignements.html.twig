<div class="modal fade" id="choix-generation-docs-divers" tabindex="-1" role="dialog" aria-labelledby="choix-generation-label">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="choix-generation-label">Divers renseignements</h4>
      </div>
      <div class="modal-body">
        <form target="_blank" id="choice-gen-divers" action="{{path('generate_divers_rens',{'idContact':contact.id})}}" method="post">
          <div class="row">
            <div class="col-md-12">
              <div class="form-group clearfix">
                  <label for="txtObjet">Objet</label>
                  <input type="text" id="txtObjet"name="txtObjet" />
              </div>
              <div class="form-group clearfix">
                <label for="txtPieces">Pièces à fournir</label>
                <textarea id="txtPieces" name="txtPieces" ></textarea>
              </div>
              <div class="form-group clearfix">
                <label for="txtRenseignements">Renseignements à donner</label>
                <textarea id="txtRenseignements" name="txtRenseignements" ></textarea>
              </div>
              <div class="form-group clearfix">
                <label for="txtFormalites">Formalités à accomplir</label>
                <textarea id="txtFormalites" name="txtFormalites" ></textarea>
              </div>
            </div>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Fermer</button>
        <button type="button" class="btn btn-primary" onclick="$('form#choice-gen-divers').submit()">Enregistrer</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->